    "surge": "^0.18.0",
    "webpack-stream": "^3.2.0",
    "wiredep": "^4.0.0",
    "yargs": "^7.0.1"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true,
      "browser": true
    },
    "rules": {
      "quotes": [
        2,
        "single"
